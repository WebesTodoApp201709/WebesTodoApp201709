
@{
    Layout = null;
    List<string> bevasarloLista = (List<string>)ViewData["bevasarloLista"];
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <ul>
            @foreach (var elem in bevasarloLista)
            {
                <li>@elem</li>
            }
        </ul>
    </div>
</body>
</html>
